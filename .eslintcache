/button>\r\n\r\n                </form>\r\n                <p>\r\n                    By signing-in you agree to Vineet's fake\r\n                    AMAZON-CLONE terms and condition of use\r\n                    & sale.Please see our Privacy Notice,our\r\n                    Cookies Notice and our Interest-Based\r\n                    Ads Notice.\r\n                </p>\r\n                <button onClick={register} className=\"login__registerButton\">Create Your Amazon Account</button>\r\n\r\n            </div>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default Login\r\n","C:\\Users\\DADDY\\Documents\\Web Dovelopment\\web dovelopment\\amazon-clone\\amazon-clone\\src\\firebase.js",[],"C:\\Users\\DADDY\\Documents\\Web Dovelopment\\web dovelopment\\amazon-clone\\amazon-clone\\src\\Payment.js",["96","97"],"import { useElements, useStripe } from '@stripe/react-stripe-js';\r\nimport React, { useState, useEffect } from 'react'\r\nimport { Link, useHistory } from 'react-router-dom';\r\nimport CurrencyFormat from \"react-currency-format\";\r\nimport CheckoutProduct from './CheckoutProduct';\r\nimport './Payment.css';\r\nimport { useStateValue } from \"./StateProvider\";\r\nimport { CardElement } from \"@stripe/react-stripe-js\";\r\nimport { getBasketTotal } from \"./reducer\";\r\nimport axios from './axios';\r\n\r\n\r\nfunction Payment(event) {\r\n    const [{ basket, user }, dispatch] = useStateValue();\r\n    const history = useHistory();\r\n\r\n    const stripe = useStripe();\r\n    const elements = useElements();\r\n\r\n    const [succeeded, setSucceeded] = useState(false);\r\n    const [processing, setProcessing] = useState(\"\");\r\n\r\n    const [error, setError] = useState(null);\r\n    const [disabled, setDisabled] = useState(true);\r\n    const [clientSecret, setClientSecret] = useState(true);\r\n    useEffect(() => {\r\n        const getClientSecret = async () => {\r\n            const response = await axios({\r\n                method: 'post',\r\n            